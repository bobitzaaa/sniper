local Players = game:GetService('Players')
local ts = game:GetService("TeleportService")
local placeId = 15502339080  -- ID-ul jocului Plaza
local minimumPlayers = 25  -- Numărul minim de jucători dorit

local function checkAndJump()
    local numPlayers = #Players:GetPlayers()
    if numPlayers < minimumPlayers then
        wait(5)  -- Așteptăm 5 secunde pentru a da timp serverelor să se actualizeze
        ts:Teleport(placeId, Players.LocalPlayer)
    end
end

Players.PlayerRemoving:Connect(function()
    checkAndJump()
end)

-- Verificăm numărul de jucători la intervale regulate
while wait(60) do
    checkAndJump()
end
