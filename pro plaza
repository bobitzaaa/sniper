task.wait(150)
    -- Coordonatele pentru cele două locații
    local coords2 = Vector3.new(630.8226928710938, 58.210540771484375, -2522.4541015625)

    -- Obținerea personajului jucătorului local
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

    -- Funcție pentru teleportare la coordonate specifice
    local function teleport(coords)
        if humanoidRootPart then
            humanoidRootPart.CFrame = CFrame.new(coords)
        else
            warn("HumanoidRootPart nu a fost găsit.")
        end
    end

    -- Funcție pentru a începe loop-ul de teleportare
    local function startTeleportLoop()
        while true do -- începe un loop infinit
            teleport(coords2)
            wait(180) -- Așteaptă 15 secunde
        end
    end

    -- Începe loop-ul imediat
    startTeleportLoop()
end)





