-- SecÈ›iunea 1: Runes + Research at selected Tier!
local MainSection1 = MainTab:CreateSection("Runes + Research at selected Tier!")

-- Coordonatele pentru teleportare iniÈ›ialÄƒ
local initialTeleportCoordinates1 = Vector3.new(-65.184249, 7.53429079, -101.7111968)

-- Coordonatele pentru teleportare atunci cÃ¢nd tier-ul este atins
local teleportCoordinates1 = Vector3.new(88.50657, 8.534290, 84.559219)

-- VariabilÄƒ pentru a stoca tier-ul dorit
local desiredTier1 = 185

-- FuncÈ›ia pentru a obÈ›ine tier-ul curent
local function getCurrentTier1()
    local tierText = game.Workspace.Roll.Board.SurfaceGui.Tier.Text
    local tierNumber = tonumber(tierText:match("%d+")) -- Extrage doar numÄƒrul din text
    if tierNumber then
        return tierNumber
    else
        return 0 -- ReturneazÄƒ 0 dacÄƒ textul nu poate fi convertit Ã®ntr-un numÄƒr
    end
end

-- FuncÈ›ia pentru a verifica tier-ul È™i a te teleporta dacÄƒ este necesar
local function checkTierAndTeleport1()
    local currentTier = getCurrentTier1()
    if currentTier >= desiredTier1 then
        local player = game.Players.LocalPlayer
        local character = player.Character
        if not character or not character.PrimaryPart then return end
        
        -- TeleporteazÄƒ la coordonatele specificate
        character:SetPrimaryPartCFrame(CFrame.new(teleportCoordinates1))
        
        -- AÈ™teaptÄƒ 1.5 secunde pentru a permite cÄƒderea
        wait(1.5)
        
        -- TeleporteazÄƒ Ã®napoi
        character:SetPrimaryPartCFrame(CFrame.new(initialTeleportCoordinates1))
    end
end

local tierCheckTimer1 = nil

-- FuncÈ›ia pentru a Ã®ncepe verificarea tier-ului
local function StartTierCheck1()
    tierCheckTimer1 = game:GetService("RunService").Heartbeat:Connect(function()
        checkTierAndTeleport1()
    end)
end

-- FuncÈ›ia pentru a opri verificarea tier-ului
local function StopTierCheck1()
    if tierCheckTimer1 then
        tierCheckTimer1:Disconnect()
        tierCheckTimer1 = nil
    end
end

-- CreazÄƒ un toggle pentru verificarea tier-ului
local ToggleTierCheck1 = MainTab:CreateToggle({
    Name = "Check Tier and Teleport (Runes + Research)",
    CurrentValue = false,
    Flag = "ToggleTierCheck1",
    Callback = function(Value)
        if Value then
            print("Tier check enabled!")
            StartTierCheck1()
            -- TeleporteazÄƒ la coordonatele iniÈ›iale
            local player = game.Players.LocalPlayer
            local character = player.Character
            if character and character.PrimaryPart then
                character:SetPrimaryPartCFrame(CFrame.new(initialTeleportCoordinates1))
            end
        else
            print("Tier check disabled!")
            StopTierCheck1()
        end
    end,
})

-- CreazÄƒ un slider pentru selectarea tier-ului dorit
local TierSlider1 = MainTab:CreateSlider({
    Name = "Desired Tier (Runes + Research)",
    Range = {150, 230},
    Increment = 1,
    Suffix = "Tier",
    CurrentValue = desiredTier1,
    Flag = "sliderDesiredTier1", -- Identificatorul pentru salvarea Ã®n configuraÈ›ie
    Callback = function(Value)
        desiredTier1 = Value
        print("Desired tier set to:", desiredTier1)
    end,
})

