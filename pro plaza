local MainTab = Window:CreateTab("‚öôÔ∏è Auto Pixels!", nil)
local MainSection = MainTab:CreateSection("Auto Colect!")



local pixelsObject = workspace:WaitForChild("Pixels")  -- Ob»õine obiectul "Pixels" din workspace
local autoCollectEnabled = false  -- Flag pentru colectarea automatƒÉ
local cycleRunning = false  -- Flag pentru a verifica dacƒÉ ciclul ruleazƒÉ
local rollTimer = 10  -- Timer implicit pentru Roll
local collectPixelsTimer = 10  -- Timer implicit pentru colectarea pixelilor

-- Func»õia pentru a te teleporta la fiecare copil din "Pixels"
local function teleportToAllPixelChildren()
    for _, child in pairs(pixelsObject:GetChildren()) do
        if not autoCollectEnabled then
            return  -- DacƒÉ autoCollectEnabled este fals, opre»ôte func»õia
        end
        if child:IsA("BasePart") then
            game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(child.CFrame)
            wait(0.1)  -- A»ôteaptƒÉ un scurt interval √Æntre teleportƒÉri
        end
    end
end

-- Func»õie pentru a √Æncepe teleportarea automatƒÉ
local function StartAutoCollect()
    autoCollectEnabled = true  -- ActiveazƒÉ flag-ul de colectare automatƒÉ
    if cycleRunning then return end  -- EvitƒÉ rularea ciclului dacƒÉ deja ruleazƒÉ

    cycleRunning = true
    spawn(function()
        while autoCollectEnabled do
            local startTime = tick()
            while autoCollectEnabled and (tick() - startTime < collectPixelsTimer) do
                teleportToAllPixelChildren()
                wait(1)  -- A»ôteaptƒÉ un interval √Ænainte de a verifica din nou
            end
            if autoCollectEnabled then
                local target = workspace:WaitForChild("Roll"):WaitForChild("Button"):WaitForChild("Touch")
                game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(target.CFrame)
                wait(rollTimer)  -- A»ôteaptƒÉ la Roll
            end
        end
        cycleRunning = false  -- ReseteazƒÉ flag-ul de ciclu
    end)
end

-- Func»õie pentru a opri teleportarea automatƒÉ »ôi pentru a te teleporta la "workspace.Roll.Button.Touch"
local function StopAutoCollect()
    autoCollectEnabled = false  -- DezactiveazƒÉ flag-ul de colectare automatƒÉ
    -- TeleporteazƒÉ jucƒÉtorul la "workspace.Roll.Button.Touch"
    local target = workspace:WaitForChild("Roll"):WaitForChild("Button"):WaitForChild("Touch")
    game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(target.CFrame)
end

-- CreazƒÉ un toggle pentru colectarea automatƒÉ a pixelilor
local ToggleCollect = MainTab:CreateToggle({
    Name = "Auto Collect Pixels ‚öôÔ∏è!",
    CurrentValue = false,
    Flag = "ToggleCollect",
    Callback = function(Value)
         if Value then
             print("Auto Collect enabled!")
             StartAutoCollect()
         else
             print("Auto Collect disabled!")
             StopAutoCollect()
         end
    end,
})

-- CreazƒÉ slider-ul pentru Roll Timer
local RollTimerSlider = MainTab:CreateSlider({
    Name = "Roll Timer",
    Range = {1, 60},
    Increment = 1,
    Suffix = "Seconds",
    CurrentValue = 10,
    Flag = "sliderRollTimer", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Value)
        rollTimer = Value
    end,
})

-- CreazƒÉ slider-ul pentru Collect Pixels Timer
local CollectPixelsTimerSlider = MainTab:CreateSlider({
    Name = "Collect Pixels Timer",
    Range = {1, 60},
    Increment = 1,
    Suffix = "Seconds",
    CurrentValue = 10,
    Flag = "sliderCollectPixelsTimer", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Value)
        collectPixelsTimer = Value
    end,
})

-- CreazƒÉ un buton pentru a porni »ôi opri colectarea automatƒÉ
local Button = MainTab:CreateButton({
    Name = "Toggle Auto Collect",
    Callback = function()
        if autoCollectEnabled then
            StopAutoCollect()
        else
            StartAutoCollect()
        end
    end,
})








