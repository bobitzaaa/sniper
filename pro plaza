local Players = game:GetService('Players')
local ts = game:GetService("TeleportService")
local placeId = 15502339080  -- ID-ul jocului Plaza

-- Funcția de verificare a numărului de jucători și inițierea teleportării
local function checkAndJump()
    local numPlayers = #Players:GetPlayers()
    if numPlayers < 20 then
        ts:Teleport(placeId, Players.LocalPlayer)  -- Teleportează jucătorul la același joc (Plaza)
    end
end

-- Setăm evenimentul pentru a verifica numărul de jucători atunci când unul părăsește jocul
Players.PlayerRemoving:Connect(checkAndJump)

-- Verificăm numărul de jucători imediat când scriptul este rulat
checkAndJump()
